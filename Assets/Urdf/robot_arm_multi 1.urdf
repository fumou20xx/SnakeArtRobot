    <cylinder length="0.2" radius="0.1" />
   </geometry>
   <origin rpy="0 0 0" xyz="0 0 0.1" />
  </collision>
  <!--inertialattribute-->
  <inertial>
   <mass value="1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.1" />
   <inertia ixx="0.02146" iyy="0.02146" izz="0.00125" ixy="0" ixz="0" iyz="0" />
  </inertial>
 </link>
 <!--torso and lowerarm connect joint-->
 <joint name="shoulder" type="revolute">
