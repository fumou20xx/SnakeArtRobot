<?xml version="1.0"?>
<robot name="robot_arm">

 <!--world-->
 <link name="world" />

 <!--world and baselink connect joint-->
 <joint name="fixed" type="fixed">
  <parent link="world" />
  <child link="base_link" />
  <child link="base_link1" />
 </joint>

 <!--baselink-->
 <link name="base_link">
  <!--visualize-->
  <visual>
   <geometry>
    <box size="0.4 0.4 0.1" />
   </geometry>
   <color rgba="0.011 0.172 0.074 1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.05" />
  </visual>
  <!--collisionattribute-->
  <collision>
   <geometry>
    <box size="0.4 0.4 0.1" />
   </geometry>
   <origin rpy="0 0 0" xyz="0 0 0.05" />
  </collision>
  <!--inertialattribute-->
  <inertial>
   <mass value="1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.05" />
   <inertia ixx="0.00354" iyy="0.00354" izz="0.00667" ixy="0" ixz="0" iyz="0" />
  </inertial>
 </link>
<!--baselink and torso connect joint-->
 <joint name="hip" type="revolute">
  <parent link="base_link" />
  <child link="torso" />
  <origin rpy="0 0 0" xyz="0.0 0.0 0.1" />
  <axis xyz="0 1 0" />
  <limit lower="0.00" upper="0.00" effort="0.5293" velocity="6.159937255" />
 </joint>

 <!--torso-->
 <link name="torso">
  <!--visualize-->
  <visual>
   <geometry>
    <cylinder length="0.2" radius="0.1" />
   </geometry>
    <color rgba="0.011 0.172 0.074 1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.1" />
  </visual>
  <!--collisionattribute-->
