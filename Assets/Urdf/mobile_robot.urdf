<?xml version="1.0" ?>
<robot name="mobile_robot">
	
  <!--basefootprint-->
  <link name="base_link">
    <!--visuarise-->
    <visual>
      <geometry>
        <box size="0.3 0.3 0.23"/>
      </geometry>
      <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
      </material>
    </visual>
    <!--collisionattribute-->
    <collision>
      <geometry>
        <box size="0.3 0.3 0.23"/>
      </geometry>
    </collision>
    <!--inertialattribute-->
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.01191" ixy="0" ixz="0" iyy="0.01191" iyz="0" izz="0.01500"/>
    </inertial>
  </link>

  <link name="left_wheel_1">
    <!--visualize-->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <!--collisionattribute-->
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <!--inertialattribute-->
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0" izz="6.125e-5"/>
    </inertial>
  </link>
  <!--baselink and leftwheel joint-->
  <joint name="left_wheel1_joint" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="left_wheel_1"/>
    <origin rpy="-1.5708 0 0" xyz="0.09 0.125 -0.09"/>
  </joint>

  <link name="left_wheel_2">
    <!--visualize-->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <!--collisionattribute-->
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <!--inertialattribute-->
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0" izz="6.125e-5"/>
    </inertial>
  </link>
  <!--baselink and leftwheel joint-->
  <joint name="left_wheel2_joint" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="left_wheel_2"/>
    <origin rpy="-1.5708 0 0" xyz="-0.09 0.125 -0.09"/>
  </joint>

  <link name="right_wheel_1">
    <!--visualize-->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <!--collisionattribute-->
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <!--inertialattribute-->
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0" izz="6.125e-5"/>
    </inertial>
  </link>
  <!--baselink and rightwheel joint-->
  <joint name="right_wheel1_joint" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="right_wheel_1"/>
    <origin rpy="-1.5708 0 0" xyz="0.09 -0.125 -.09"/>
  </joint>

  <link name="right_wheel_2">
    <!--visualize-->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <!--collisionattribute-->
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.035"/>
      </geometry>
    </collision>
    <!--inertialattribute-->
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0" izz="6.125e-5"/>
    </inertial>
  </link>
  <!--baselink and rightwheel joint-->
  <joint name="right_wheel2_joint" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="right_wheel_2"/>
    <origin rpy="-1.5708 0 0" xyz="-0.09 -0.125 -.09"/>
  </joint>

  <!--setting powered by sensor-->
    <link name="base_scan">
      <!--visualize-->
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.1 0.1 0.1"/>
        </geometry>
      </visual>
      <!--collisionattribute-->
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.1 0.1 0.1"/>
        </geometry>
      </collision>
      <!--inertialattribute-->
      <inertial>
        <mass value="1e-5"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
      </inertial>
    </link>
    <!--baselink and sensor_link joint-->
    <joint name="sensor_joint" type="fixed">
      <axis xyz="0 1 0"/>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <parent link="base_link"/>
      <child link="base_scan"/>
    </joint>

</robot>

