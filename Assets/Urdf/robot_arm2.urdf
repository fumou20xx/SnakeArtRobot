<?xml version="1.0"?>
<robot name="robot_arm">

 <!--world-->
 <link name="world" />

 <!--world and baselink connect joint-->
 <joint name="fixed" type="fixed">
  <parent link="world" />
  <child link="base_link" />
 </joint>

 <!--baselink-->
 <link name="base_link">
  <!--visualize-->
  <visual>
   <geometry>
    <box size="0.2 0.2 0.05" />
   </geometry>
   <material name="white">
    <color rgba="1.0 1.0 1.0 1.0" />
   </material>
   <origin rpy="0 0 0" xyz="0 0 0.025" />
  </visual>

  <!--collisionattribute-->
  <collision>
   <geometry>
    <box size="0.2 0.2 0.05" />
   </geometry>
   <origin rpy="0 0 0" xyz="0 0 0.025" />
  </collision>

  <!--inertialattribute-->
  <inertial>
   <mass value="1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.025" />
   <inertia ixx="0.00354" iyy="0.00354" izz="0.00667" ixy="0" ixz="0" iyz="0" />
  </inertial>
 </link>
<!--baselink and torso connect joint-->
 <joint name="hip" type="revolute">
  <parent link="base_link" />
  <child link="torso" />
  <origin rpy="0 0 0" xyz="0.0 0.0 0.05" />
  <axis xyz="0 1 0" />
  <limit lower="-1.00" upper="0.00" effort="0.5293" velocity="6.159937255" />
 </joint>

 <!--torso-->
 <link name="torso">
  <!--visualize-->
  <visual>
   <geometry>
    <cylinder length="0.5" radius="0.05" />
   </geometry>
   <material name="white" />
   <origin rpy="0 0 0" xyz="0 0 0.25" />
  </visual>

  <!--collisionattribute-->
  <collision>
   <geometry>
    <cylinder length="0.5" radius="0.05" />
   </geometry>
   <origin rpy="0 0 0" xyz="0 0 0.25" />
  </collision>

  <!--inertialattribute-->
  <inertial>
   <mass value="1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.25" />
   <inertia ixx="0.02146" iyy="0.02146" izz="0.00125" ixy="0" ixz="0" iyz="0" />
  </inertial>
 </link>

 <!--torso and upperarm connect joint-->
 <joint name="shoulder" type="revolute">
  <parent link="torso" />
  <child link="upper_arm" />
  <origin rpy="0 0 0" xyz="0.0 -0.1 0.45" />
  <axis xyz="0 1 0" />
  <limit lower="-0.00" upper="2.00" effort="0.5293" velocity="6.159937255" />
 </joint>
<!--upperarm-->
 <link name="upper_arm">
  <!--visualize-->
  <visual>
   <geometry>
    <cylinder length="0.4" radius="0.05" />
   </geometry>
   <material name="white" />
   <origin rpy="0 0 0" xyz="0 0 0.2" />
  </visual>

  <!--collisionattribute-->
  <collision>
   <geometry>
    <cylinder length="0.4" radius="0.05" />
   </geometry>
   <origin rpy="0 0 0" xyz="0 0 0.2" />
  </collision>

  <!--inertialattribute-->
  <inertial>
   <mass value="1.0" />
   <origin rpy="0 0 0" xyz="0 0 0.2" />
   <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0" />
  </inertial>
 </link>

</robot>
